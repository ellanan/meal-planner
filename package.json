    "autoprefixer": "^9.8.8",
    "concurrently": "^6.3.0",
    "postcss": "^7.0.39",
    "prisma": "^3.5.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.17",
    "ts-node": "^10.2.1",
    "vercel": "^23.1.2"
  },
  "volta": {
    "node": "16.13.0",
    "npm": "8.1.4"
  }
}
