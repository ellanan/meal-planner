{
  "name": "meal-planner",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "concurrently \"node scripts/ngrok.js\" \"vercel dev\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "db:reset": "npx prisma db push && npm run db:seed",
    "db:seed": "npx ts-node prisma/seed.ts",
    "dev:server": "npx prisma generate --watch",
    "client-codegen": "graphql-codegen --config codegen.yml",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.4.15",
    "@chakra-ui/react": "^1.6.10",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@graphql-codegen/cli": "^2.2.0",
    "@prisma/client": "^3.1.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "apollo-server-micro": "^3.3.0",
    "graphql": "^15.6.0",
    "lodash": "^4.17.21",
    "luxon": "^2.0.2",
    "micro-cors": "^0.1.1",
    "next": "^11.1.2",
    "nexus": "^1.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "react-select": "^5.1.0",
    "typescript": "^4.4.3",
    "web-vitals": "^1.0.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.2.0",
    "@graphql-codegen/typescript": "2.2.2",
    "@graphql-codegen/typescript-document-nodes": "2.1.4",
    "@graphql-codegen/typescript-operations": "2.1.6",
    "@graphql-codegen/typescript-react-apollo": "^2.1.4",
    "@types/lodash": "^4.14.175",
    "@types/luxon": "^2.0.5",
    "@types/micro-cors": "^0.1.2",
    "@types/react-router-dom": "^5.3.0",
    "concurrently": "^6.3.0",
    "ngrok": "^4.2.2",
    "prisma": "^3.1.1",
    "ts-node": "^10.2.1",
    "vercel": "^23.1.2"
  }
}
