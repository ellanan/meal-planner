{
  "functions": {
    "api/**/**": {
      "includeFiles": "api/tsconfig.json"
    }
  },
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "rewrites": [
    {
      "source": "/((?!@vite|src|@react-refresh|node_modules|public|manifest|logo).*)",
      "destination": "/index.html"
    }
  ]
}
